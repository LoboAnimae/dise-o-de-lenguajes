COMPILER ArchivoPrueba4 // Ignore this

CHARACTERS

letra = "a..zA..Z". /*
also ignore this
*/
digito = "0123456789".
any = "01" + letra + digito.
notany


= any - any.
tab = CHR(9).
eol = CHR(10).

KEYWORDS

if = "if".

TOKENS

underhundred = digito[digito].
id = letra{letra|digito} EXCEPT KEYWORDS.
numero = digito{digito}.
notAny = notany.
tab = tab.
eol = eol.


END ArchivoPrueba4